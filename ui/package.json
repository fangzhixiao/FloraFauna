    "bootstrap": "^3.4.1",
    "css-loader": "^6.2.0",
    "date-fns": "^2.23.0",
    "dotenv": "^6.2.0",
    "express": "^4.17.1",
    "http-proxy-middleware": "^0.21.0",
    "isomorphic-fetch": "^2.2.1",
    "moment": "^2.29.1",
    "nodemon": "^1.19.4",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-bootstrap": "^0.33.1",
    "react-datetime": "^3.0.4",
    "react-dom": "^16.14.0",
    "react-google-maps": "^9.4.5",
    "react-router-bootstrap": "^0.25.0",
    "react-router-dom": "^4.3.1",
    "react-select": "^2.4.4",
    "serialize-javascript": "^1.9.1",
